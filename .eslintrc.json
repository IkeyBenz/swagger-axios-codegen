{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "dot-notation": "error",
        "valid-jsdoc": "off",
        "require-jsdoc": "off"
    },
    "overrides": [
        {
            "files": "./api2/*.ts"
        }
    ]
}